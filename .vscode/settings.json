{
  "editor.rulers": [
    80,
    100
  ],
  "search.exclude": {
    "**/.yarn": true,
    "**/.pnp.*": true
  },
  "csharp.semanticHighlighting.enabled": true,
  "cSpell.words": [
    "bencoded",
    "Bencodex",
    "Bitcoin",
    "Ethereum",
    "Kademlia",
    "Libplanet",
    "Libplanet's",
    "PRNG",
    "Unstage",
    "blockchain",
    "branchpoint",
    "broadcasted",
    "changeset",
    "ciphertext",
    "indeterministic",
    "marshaler",
    "nonces",
    "outdate",
    "parameterless",
    "preload",
    "roadmap",
    "ruleset",
    "runtimes",
    "secp256k1",
    "struct",
    "txid",
    "unmarshal",
    "unmarshaling",
    "unstage",
    "unstaged",
    "unrender",
    "unrendered",
    "unrenderer",
    "unrendering",
    "unrenders",
    "upstream's"
  ],
  "dotnet-test-explorer.autoExpandTree": true,
  "dotnet-test-explorer.testProjectPath": "**/*Tests.@(csproj|vbproj|fsproj)",
  "typescript.tsdk": ".yarn/sdks/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "eslint.nodePath": ".yarn/sdks",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports.rome": true
    }
  },
  "[javascript]": {
    "editor.defaultFormatter": "rome.rome",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports.rome": true
    }
  },
  "[javascript][typescript]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports.rome": "explicit"
    }
  }
}
